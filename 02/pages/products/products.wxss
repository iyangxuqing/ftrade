.cate {
  margin-bottom: 20rpx;
}

.product {
  width: 200rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.product-image {
  width: 200rpx;
  height: 200rpx;
}

.product-title {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 0.8rem;
}

.product-new .product-image {
  padding: 40rpx;
  border: 1px solid #ccc;
}
