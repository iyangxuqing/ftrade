.cate {
  display: flex;
  margin-bottom: 20rpx;
}

.cate-ptitle {
  margin-right: 10rpx;
}

.cate-title {
  margin-left: 10rpx;
}

.product-images {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.product-image {
  width: 120rpx;
  height: 120rpx;
}

.product-title {
  height: 100rpx;
  display: flex;
  align-items: center;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}

.product-title-label {
  margin-right: 20rpx;
}

.head {
  height: 72rpx;
  line-height: 72rpx;
  padding-left: 10rpx;
  background-color: #eee;
  margin-top: 40rpx;
}

.list {
  border-bottom: 1px solid #ccc;
}

.item {
  display: flex;
  align-items: center;
  height: 100rpx;
  overflow: hidden;
  border-bottom: 1px solid #ddd;
}

.item:last-child {
  border-bottom: 0;
}

.item-inner {
  position: relative;
  width: 100%;
  height: 100rpx;
  transition: all 300ms ease;
}

.item-inner.swipeleft {
  transform: translateX(-100rpx);
}

.left {
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  width: 100%;
  display: flex;
  align-items: center;
}

.right {
  position: absolute;
  right: -100rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 100rpx;
  display: flex;
  align-items: center;
}

.item-label {
  width: 50%;
}

.item-value {
  width: 50%;
}

.item-delete {
  width: 100rpx;
  height: 96rpx;
  padding: 28rpx 30rpx;
  background-color: red;
}

.foot {
  height: 200rpx;
}
