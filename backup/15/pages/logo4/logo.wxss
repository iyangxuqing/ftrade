.container{
  width: 500rpx;
  height: 100%;
  border: 2rpx solid #ccc;
  background: #ccc;
  display: flex;
  flex-direction: column;
 align-items: center;
 padding: 20rpx;
}
.body{
  width: 100%;
}
.label{
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding-bottom: 30rpx;
  border-bottom: 2rpx dashed #fff;
  align-items: center;
}
.title{
  font-size: 1.4em;
  margin-bottom: 10rpx;
}
.logo{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 40rpx;
  border-radius: 50%;
  overflow: hidden;
  display: flex;
  margin-top: 40rpx;
}
.subTitle{
  font-size: 0.9em;
  color: #000;
}
.icon{
  width: 50rpx;
  height: 50rpx;
}
.ico{
  width: 50rpx;
  height: 50rpx;
}
.item{
  width: 100%;
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  align-items: center;
}
.collection{
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx dashed #fff;
}